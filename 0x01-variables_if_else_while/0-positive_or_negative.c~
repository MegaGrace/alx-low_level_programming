#include <stdlib.h>
#include <time.h>

/**
*main: Entry point
*Description: Generates random number and displays them
*Return 0
*/

int main(void)
{
	int n;

	srand(time(0));
	n = rand() - RAND_MAX / 2;
	if (n > 0)
	{
	printf ("%i is positive\n", n);
	}
	else if (n == 0)
	{
	printf("%i is zero\n", n);
	}
	else
	{
	printf("is negative\n", n);
	}
	return (0);
}
